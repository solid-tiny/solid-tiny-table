{
  "name": "solid-tiny-table",
  "version": "0.4.0",
  "description": "A tiny table data helper for SolidJS.",
  "keywords": [
    "solidjs",
    "table"
  ],
  "author": "gmero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/solid-tiny/solid-tiny-table"
  },
  "homepage": "https://github.com/solid-tiny/solid-tiny-table#readme",
  "packageManager": "pnpm@10.12.1",
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vite -c ./playground/vite.config.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "npx ultracite lint",
    "lint:fix": "npx ultracite format",
    "tc": "tsc --noEmit --skipLibCheck",
    "bump": "bumpp --no-push",
    "prepublish": "pnpm tc && pnpm lint && pnpm build",
    "prepare": "husky"
  },
  "peerDependencies": {
    "solid-js": "^1.9.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@solidjs/router": "^0.15.3",
    "@solidjs/testing-library": "^0.8.10",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.2.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "ultracite": "5.1.2",
    "unocss": "^66.4.2",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.8",
    "vite-plugin-solid-pages": "^0.3.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "solid-tiny-context": "^0.2.3",
    "solid-tiny-utils": "^0.6.0"
  }
}
